	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Newtonsoft Json Deserialization</Title>
			<Shortcut>deser</Shortcut>
			<author>simontjoszef@gmail.com</author>
		</Header>
		<Snippet>
			<Imports>
				<!-- Use this to automatically add usings -->
				<Import>
					<Namespace>Newtonsoft.Json</Namespace>
				</Import>
			</Imports>
			<Declarations>
				<!-- Use this to add variables to snippet-->
				<Literal>
					<!--<ID>T</ID>
					<ToolTip>Generic type</ToolTip>
					<Default>T</Default>-->
				</Literal>
			</Declarations>
			<Code Language="CSharp">
				<!-- $Selected$ when using ctrl+k,s (suround with) the selected text will be inserted to this literals location -->
				<!-- $end$ after inserting the snipet, the cursor will be placed here -->
				<![CDATA[JsonConvert.DeserializeObject<T>($selected$$end$);]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>